{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "bensampo/laravel-enum": "^6.6",
        "ctf0/media-manager": "^4.0",
        "doctrine/dbal": "^3.6",
        "fruitcake/laravel-cors": "dev-develop",
        "guzzlehttp/guzzle": "^7.2",
        "laravel-json-api/laravel": "^3.0",
        "laravel/framework": "^10.0",
        "laravel/socialite": "^5.8",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^4.2",
        "pbmedia/laravel-ffmpeg": "^8.3",
        "pda/pheanstalk": "^4.0",
        "php-ffmpeg/php-ffmpeg": "^1.1",
        "predis/predis": "^2.2",
        "savannabits/primevue-datatables": "^1.5",
        "sentry/sentry-laravel": "^3.8",
        "socialiteproviders/discord": "^4.2",
        "spatie/laravel-medialibrary": "^10.11",
        "spatie/laravel-permission": "^5.10",
        "symfony/process": "^6.3",
        "tymon/jwt-auth": "^2.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.22",
        "kitloong/laravel-migrations-generator": "^6.10",
        "knuckleswtf/scribe": "^4.22",
        "laravel-json-api/testing": "^2.0",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan pcl:post-update"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": [
            "@php artisan pcl:post-install"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}